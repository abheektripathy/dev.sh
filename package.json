	"devDependencies": {
		"@types/node": "^18.16.3",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"typescript": "^5.0.2"
	},
	"dependencies": {
		"axios": "^1.4.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"openai": "^3.2.1"
