{
	"name": "devsh",
	"description": "Streamline your Developer workflow",
	"version": "0.0.1",
	"publisher": "AbheekTripathy",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/abheektripathy/dev.sh"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
